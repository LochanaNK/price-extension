{
  "manifest_version": 3,
  "name": "Price Comparator",
  "version": "1.0",
  "description": "Compare product prices across AliExpress and Daraz",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "permissions": ["storage", "activeTab", "scripting"]
}
